extend type Query {
    #查询组织机构
    #(treeType:String)
    depts(status:Int):[Dept]
    #查询岗位
    posts(status:Int):[Dept]
}



# 组织机构实体
type Dept{

    # 机构类型代码
    code:String
    # 机构类型名称
    name:String
    #父级code
    parentCode:String
    # 来源
    source:String
    #
    treeType:String
    #类型
    type:String
    #标签
    tags:String

}








